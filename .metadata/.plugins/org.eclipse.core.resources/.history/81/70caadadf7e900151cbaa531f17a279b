package game.util;

public class Vector2i {
	
	private int x, y;
	
	public Vector2i(int x, int y){
		this.x = x;
		this.y = y;
	}
	
	public void test(){
		Vector2i position = new Vector2i(80,40).setX(15);
	}
	
	public int getX(){
		return x;
	}
	public int getY(){
		return y;
	}
	
	public Vector2i setX(int x){
		this.x = x;
	}
	public void setY(int y){
		this.x = y;
	}
}
